{
  "cli": {
    "version": ">= 5.2.0",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "buildConfiguration": "Debug"
      },
      "env": {
        "TAMAGUI_TARGET": "native",
        "NO_FLIPPER": "1"
      },
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "node": "18.14.0",
      "env": {
        "TAMAGUI_TARGET": "native"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
