{
  "name": "2048tris",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "supabase"
  ],
  "scripts": {
    "_with-env": "dotenv -e .env -c -- ",
    "ios": "cd apps/expo && yarn ios",
    "android": "cd apps/expo && yarn android",
    "native": "cd apps/expo && yarn start",
    "watch": "yarn workspaces foreach --all -pi run watch",
    "web": "yarn build && cd apps/next && yarn dev",
    "web:extract": "DISABLE_EXTRACTION=false yarn workspace next-app dev",
    "web:prod": "yarn workspace next-app build",
    "web:prod:analyze": "ANALYZE=true yarn web:prod",
    "web:prod:serve": "yarn workspace next-app serve",
    "fix": "manypkg fix",
    "postinstall": "yarn check-deps && yarn build",
    "build": "yarn workspaces foreach --all --exclude next-app --exclude storybook-app run build",
    "storybook": "yarn storybook:web",
    "sb": "yarn storybook:web",
    "storybook:web": "yarn build && cd apps/storybook && yarn dev",
    "storybook:native": "yarn build && cd apps/storybook-rn && yarn storybook",
    "storybook:ios": "yarn build && cd apps/storybook-rn && yarn ios",
    "storybook:android": "yarn build && cd apps/storybook-rn && yarn android",
    "chromatic": "yarn build && cd apps/storybook && yarn chromatic",
    "upgrade:tamagui": "yarn up '*tamagui*' '@tamagui/*' react-native-web-lite",
    "upgrade:tamagui:canary": "yarn up '*tamagui*'@canary '@tamagui/*'@canary react-native-web-lite@canary",
    "check-deps": "check-dependency-version-consistency .",
    "supa": "yarn workspace @my/supabase",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "setup": "yarn create tamagui --template takeout-starter --skip-cloning",
    "generate": "turbo gen",
    "gen": "yarn generate"
  },
  "resolutions": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-refresh": "^0.14.0",
    "react-native-svg": "13.9.0",
    "react-native-web": "~0.19.6",
    "@babel/core": "^7.20.2",
    "babel-loader": "^8.3.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "@manypkg/cli": "^0.19.1",
    "@nderscore/tamagui-typescript-plugin": "^0.5.1",
    "@tamagui/cli": "^1.74.8",
    "check-dependency-version-consistency": "^3.0.3",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.46.0",
    "expo-dev-client": "~2.4.11",
    "node-gyp": "^9.3.1",
    "prettier": "^2.7.1",
    "turbo": "^1.10.14",
    "typescript": "^5.1.3"
  },
  "packageManager": "yarn@4.0.0",
  "devDependencies": {
    "@turbo/gen": "^1.10.12",
    "change-case": "^4.1.2"
  }
}
